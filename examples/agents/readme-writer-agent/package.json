{
  "name": "readme-writer-agent",
  "version": "0.1.0",
  "description": "An AI agent to help you write README files for open-source projects.",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "write-readme": "dist/index.js"
  },
