  "dependencies": {
    "@baseai/core": "^0.9.20",
    "@clack/prompts": "^0.7.0",
    "chalk": "5.6.0",
    "clear-any-console": "^1.16.2",
    "figures": "^6.1.0",
    "picocolors": "^1.1.0",
    "tsup": "^8.3.0"
  },
  "devDependencies": {
    "baseai": "^0.9.20"
  }
