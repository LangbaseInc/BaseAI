{
	"$schema": "https://turbo.build/schema.json",
	"globalEnv": [
		"CI",
		"PORT"
	],
	"tasks": {
		"build": {
			"dependsOn": [
				"^build"
			],
			"env": [
				"ANTHROPIC_API_KEY",
				"AWS_REGION",
				"AWS_ACCESS_KEY_ID",
				"AWS_SECRET_ACCESS_KEY",
				"COHERE_API_KEY",
				"FIREWORKS_API_KEY",
				"GOOGLE_API_KEY",
				"GROQ_API_KEY",
				"HUGGINGFACE_API_KEY",
				"MISTRAL_API_KEY",
				"OPENAI_API_KEY",
				"OPENAI_API_BASE",
				"PERPLEXITY_API_KEY",
				"REPLICATE_API_KEY",
				"NODE_ENV",
				"ASSISTANT_ID",
				"INKEEP_API_KEY",
				"INKEEP_INTEGRATION_ID",
				"VERCEL_URL"
			],
			"outputs": [
				"**/dist/**",
				".next/**",
				"!.next/cache/**"
			]
		},
		"lint": {
			"dependsOn": [
				"^lint"
			]
		},
		"type-check": {
			"dependsOn": [
				"^build",
				"build"
			]
		},
		"test": {
			"dependsOn": [
				"^build",
				"build"
			]
		},
		"publint": {
			"dependsOn": [
				"^build",
				"build"
			]
		},
		"clean": {
			"dependsOn": [
				"^clean"
			]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"prettier-check": {},
		"integration-test": {
			"dependsOn": [
				"^build",
				"build"
			]
		}
	}
}
